<tryton>
    <data>
        <!-- Tipo Comision -->
        <record model="ir.ui.view" id="tipo_comision_view_form">
            <field name="model">corseg.tipo_comision</field>
            <field name="type">form</field>
            <field name="name">tipo_comision_form</field>
        </record>
        <record model="ir.ui.view" id="tipo_comision_view_tree">
            <field name="model">corseg.tipo_comision</field>
            <field name="type">tree</field>
            <field name="name">tipo_comision_tree</field>
        </record>
        <record model="ir.action.act_window" id="act_tipo_comision_form">
            <field name="name">Tipo Comision</field>
            <field name="res_model">corseg.tipo_comision</field>
        </record>
        <record model="ir.action.act_window.view" id="act_tipo_comision_form_view1">
            <field name="sequence" eval="10"/>
            <field name="view" ref="tipo_comision_view_tree"/>
            <field name="act_window" ref="act_tipo_comision_form"/>
        </record>
        <record model="ir.action.act_window.view" id="act_tipo_comision_form_view2">
            <field name="sequence" eval="20"/>
            <field name="view" ref="tipo_comision_view_form"/>
            <field name="act_window" ref="act_tipo_comision_form"/>
        </record>

        <menuitem parent="menu_corseg_configuration" sequence="25"
            action="act_tipo_comision_form" id="menu_tipo_comision_tree"/>

        <record model="ir.model.access" id="access_tipo_comision">
            <field name="model" search="[('model', '=', 'corseg.tipo_comision')]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_delete" eval="False"/>
        </record>
        <record model="ir.model.access" id="access_tipo_comision_admin">
            <field name="model" search="[('model', '=', 'corseg.tipo_comision')]"/>
            <field name="group" ref="group_corseg_admin"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>


        <!-- Comision Cia -->
        <record model="ir.ui.view" id="comision_cia_view_form">
            <field name="model">corseg.comision.cia</field>
            <field name="type">form</field>
            <field name="name">comision_cia_form</field>
        </record>
        <record model="ir.ui.view" id="comision_cia_view_tree">
            <field name="model">corseg.comision.cia</field>
            <field name="type">tree</field>
            <field name="name">comision_cia_tree</field>
        </record>
        <record model="ir.model.access" id="access_comision_cia">
            <field name="model" search="[('model', '=', 'corseg.comision.cia')]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_delete" eval="False"/>
        </record>
        <record model="ir.model.access" id="access_comision_cia_admin">
            <field name="model" search="[('model', '=', 'corseg.comision.cia')]"/>
            <field name="group" ref="group_corseg_admin"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>


        <!-- Comision Vendedor -->
        <record model="ir.ui.view" id="comision_vendedor_view_form">
            <field name="model">corseg.comision.vendedor</field>
            <field name="type">form</field>
            <field name="name">comision_vendedor_form</field>
        </record>
        <record model="ir.ui.view" id="comision_vendedor2_view_form">
            <field name="model">corseg.comision.vendedor</field>
            <field name="type">form</field>
            <field name="name">comision_vendedor2_form</field>
        </record>
        <record model="ir.ui.view" id="comision_vendedor_view_tree">
            <field name="model">corseg.comision.vendedor</field>
            <field name="type">tree</field>
            <field name="name">comision_vendedor_tree</field>
        </record>
        <record model="ir.ui.view" id="comision_vendedor2_view_tree">
            <field name="model">corseg.comision.vendedor</field>
            <field name="type">tree</field>
            <field name="name">comision_vendedor2_tree</field>
        </record>
        <record model="ir.model.access" id="access_comision_vendedor">
            <field name="model" search="[('model', '=', 'corseg.comision.vendedor')]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_delete" eval="False"/>
        </record>
        <record model="ir.model.access" id="access_comision_vendedor_admin">
            <field name="model" search="[('model', '=', 'corseg.comision.vendedor')]"/>
            <field name="group" ref="group_corseg_admin"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>
    </data>
</tryton>

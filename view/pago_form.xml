<?xml version="1.0"?>
<!-- This file is part of trytond-corseg module. The COPYRIGHT file at the top level of
this repository contains the full copyright notices and license terms. -->
<form>
    <label name="fecha"/>
    <field name="fecha"/>
    <label name="number"/>
    <field name="number"/>
    <label name="referencia"/>
    <field name="referencia"/>
    <newline/>
    <label name="poliza"/>
    <field name="poliza"/>
    <label name="cia"/>
    <field name="cia"/>
    <label name="contratante"/>
    <field name="contratante"/>
    <label name="renovacion"/>
    <field name="renovacion"/>
    <label name="vendedor"/>
    <field name="vendedor"/>
    <newline />
    <label name="monto"/>
    <field name="monto"/>
    <newline />
    <label name="comision_cia_liq"/>
    <field name="comision_cia_liq"/>
    <label name="comision_vendedor_liq"/>
    <field name="comision_vendedor_liq"/>
    <notebook colspan="4">
        <page string="Comisiones" id="comisiones">
            <group string="Cia" id="com_cia" colspan="2" col="2">
                <label name="comision_cia"/>
                <field name="comision_cia"/>
                <label name="comision_cia_sugerida"/>
                <field name="comision_cia_sugerida"/>
                <label name="comision_cia_ajuste"/>
                <field name="comision_cia_ajuste"/>
            </group>
            <group string="Vendedor" id="com_vendedor" colspan="2" col="2">
                <label name="comision_vendedor"/>
                <field name="comision_vendedor"/>
                <label name="comision_vendedor_sugerida"/>
                <field name="comision_vendedor_sugerida"/>
                <label name="comision_vendedor_ajuste"/>
                <field name="comision_vendedor_ajuste"/>
            </group>
            <separator string="Ajustes" id="com_ajustes" colspan="4"/>
            <field name="ajustes_comision_cia" colspan="2"/>
            <field name="ajustes_comision_vendedor" colspan="2"/>
        </page>
        <page string="Comentarios" id="comentario">
            <field name="comentario" colspan="4"/>
        </page>
        <page string="Sustitucion" id="sustitucion">
            <label name="sustituir"/>
            <field name="sustituir"/>
            <label name="pago_sustituir"/>
            <field name="pago_sustituir"/>
            <label name="sustituido_por"/>
            <field name="sustituido_por"/>
        </page>
        <page string="Info" id="info">
            <label name="company"/>
            <field name="company"/>
            <label name="currency"/>
            <field name="currency"/>
            <separator string="Liquidacion de Comisiones" id="liquidacion" colspan="4"/>
            <label name="liq_cia"/>
            <field name="liq_cia"/>
            <label name="liq_vendedor"/>
            <field name="liq_vendedor"/>
            <separator string="Auditoria" id="auditoria" colspan="4"/>
            <label name="made_by"/>
            <field name="made_by"/>
            <label name="made_date"/>
            <field name="made_date"/>
            <label name="processed_by"/>
            <field name="processed_by"/>
            <label name="processed_date"/>
            <field name="processed_date"/>
            <label name="confirmed_by"/>
            <field name="confirmed_by"/>
            <label name="confirmed_date"/>
            <field name="confirmed_date"/>
            <label name="canceled_by"/>
            <field name="canceled_by"/>
            <label name="canceled_date"/>
            <field name="canceled_date"/>
        </page>
    </notebook>
    <group col="6" colspan="4" id="buttons" yfill="1">
        <label name="state"/>
        <field name="state"/>
        <button name="cancelar" string="Cancelado"
            icon="tryton-cancel"/>
        <button name="borrador" string="Borrador"/>
        <button name="procesar" string="Procesar"
            icon="tryton-ok"/>
        <button name="confirmar" string="Confirmar"
            icon="tryton-go-next"/>
    </group>
</form>

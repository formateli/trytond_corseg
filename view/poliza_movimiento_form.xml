this repository contains the full copyright notices and license terms. -->
<form>
    <label name="fecha"/>
    <field name="fecha"/>
    <label name="poliza"/>
    <field name="poliza"/>
    <label name="descripcion"/>
    <field name="descripcion" colspan="3"/>
    <label name="tipo"/>
    <field name="tipo"/>
    <label name="tipo_endoso"/>
    <field name="tipo_endoso"/>
    <newline />
    <notebook colspan="4">
        <page string="Datos" id="datos" col="6">
            <label name="contratante"/>
            <field name="contratante"/>
            <newline />
            <label name="f_emision"/>
            <field name="f_emision"/>
            <label name="f_desde"/>
            <field name="f_desde"/>
            <label name="f_hasta"/>
            <field name="f_hasta"/>
            <label name="suma_asegurada"/>
            <field name="suma_asegurada"/>
            <label name="prima"/>
            <field name="prima"/>
            <newline />
            <label name="forma_pago"/>
            <field name="forma_pago"/>
            <label name="frecuencia_pago"/>
            <field name="frecuencia_pago"/>
            <label name="no_cuotas"/>
            <field name="no_cuotas"/>
        </page>
        <page string="Inclusiones / Exclusiones" id="inclu_exclu">
            <field name="inclu_exclu" colspan="6"/>
        </page>
        <page string="Comisiones" id="comisiones">
            <label name="vendedor"/>
            <field name="vendedor"/>
        </page>
        <page string="Comentarios" id="comentario">
            <field name="comentario" colspan="4"/>
        </page>
    </notebook>

    <group col="6" colspan="4" id="buttons" yfill="1">
        <label name="state"/>
        <field name="state"/>
        <button name="cancelar" string="Cancelado"
            icon="tryton-cancel"/>
        <button name="borrador" string="Borrador"/>
        <button name="procesar" string="Procesar"
            icon="tryton-ok"/>
        <button name="confirmar" string="Confirmar"
            icon="tryton-go-next"/>
    </group>
</form>

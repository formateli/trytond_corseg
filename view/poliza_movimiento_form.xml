<form>
    <label name="poliza"/>
    <field name="poliza" colspan="3"/>
    <label name="fecha"/>
    <field name="fecha"/>
    <label name="descripcion"/>
    <field name="descripcion"/>
    <label name="tipo"/>
    <field name="tipo"/>
    <label name="vendedor"/>
    <field name="vendedor"/>
    <newline />
    <notebook colspan="4">
        <page string="Endoso" id="endoso">
            <label name="renovacion"/>
            <field name="renovacion"/>
            <label name="finiquito"/>
            <field name="finiquito"/>
            <label name="f_emision"/>
            <field name="f_emision"/>
            <label name="contratante"/>
            <field name="contratante"/>
            <label name="f_desde"/>
            <field name="f_desde"/>
            <label name="f_hasta"/>
            <field name="f_hasta"/>
            <label name="suma_asegurada"/>
            <field name="suma_asegurada"/>
            <label name="prima"/>
            <field name="prima"/>
            <separator string="Pago" colspan="4"/>
            <label name="forma_pago"/>
            <field name="forma_pago"/>
            <label name="frecuencia_pago"/>
            <field name="frecuencia_pago"/>
            <label name="no_cuotas"/>
            <field name="no_cuotas"/>
            <newline/>
            <separator string="Inclusiones / Exclusiones" colspan="4"/>
            <field name="inclusion" colspan="2"/>
            <field name="exclusion" colspan="2"/>
        </page>
        <page string="Comentarios" id="comentario">
            <field name="comentario" colspan="4"/>
        </page>
    </notebook>
</form>

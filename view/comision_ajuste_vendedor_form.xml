<form>
    <label name="fecha"/>
    <field name="fecha"/>
    <label name="number"/>
    <field name="number"/>
    <label name="pago"/>
    <field name="pago"/>
    <newline/>
    <label name="monto"/>
    <field name="monto"/>
    <notebook colspan="4">
        <page string="Comentarios" id="comentario">
            <field name="comentario" colspan="4"/>
        </page>
        <page string="Info" id="info">
            <label name="company"/>
            <field name="company"/>
            <label name="currency"/>
            <field name="currency"/>
            <separator string="Auditoria" id="auditoria" colspan="4"/>
            <label name="made_by"/>
            <field name="made_by"/>
            <label name="made_date"/>
            <field name="made_date"/>
        </page>
    </notebook>
    <group col="6" colspan="4" id="buttons" yfill="1">
        <label name="state"/>
        <field name="state"/>
        <button name="borrador" string="Borrador"/>
        <button name="finalizar" string="Finalizar"
            icon="tryton-go-next"/>
    </group>
</form>

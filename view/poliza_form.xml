<form>
    <label name="cia"/>
    <field name="cia"/>
    <label name="cia_producto"/>
    <field name="cia_producto"/>
    <label name="numero"/>
    <field name="numero"/>
    <label name="ramo"/>
    <field name="ramo"/>
    <label name="origen"/>
    <field name="origen"/>
    <label name="grupo"/>
    <field name="grupo"/>
    <label name="contratante"/>
    <field name="contratante"/>
    <label name="vendedor"/>
    <field name="vendedor"/>
    <newline />
    <notebook colspan="4">
        <page string="Informacion" id="informacion" col="6">
            <group string="Vigencia actual" colspan="6" col="6" id="emision">
                <label name="renovacion"/>
                <field name="renovacion"/>
                <label name="state"/>
                <field name="state"/>
                <newline/>
                <label name="f_emision"/>
                <field name="f_emision"/>
                <label name="f_desde"/>
                <field name="f_desde"/>
                <label name="f_hasta"/>
                <field name="f_hasta"/>
                <label name="suma_asegurada"/>
                <field name="suma_asegurada"/>
                <label name="prima"/>
                <field name="prima"/>
                <label id="after_prima" colspan="2"/>
                <label id="before_total" colspan="4"/>
                <label name="total"/>
                <field name="total"/>
            </group>
            <group string="Pagos" colspan="6" col="6" id="pago">
                <label name="forma_pago"/>
                <field name="forma_pago"/>
                <label name="frecuencia_pago"/>
                <field name="frecuencia_pago"/>
                <label name="no_cuotas"/>
                <field name="no_cuotas"/>
                <label name="cuota"/>
                <field name="cuota"/>
                <label name="monto_pago"/>
                <field name="monto_pago"/>
                <label name="saldo"/>
                <field name="saldo"/>
            </group>
            <separator string="Notas" colspan="6" id="notas"/>
            <field name="notas" colspan="6"/>
        </page>
        <page string="Renovaciones" id="renovaciones">
            <field name="renovaciones" colspan="4"/>
        </page>
        <page string="Certificados" id="certificados">
            <field name="certificados_in" colspan="2"/>
            <field name="certificados_ex" colspan="2"/>
        </page>
        <page string="Movimientos" id="movimientos">
            <field name="movimientos"/>
        </page>
        <page string="Pagos" id="pagos">
            <field name="pagos" colspan="4"/>
        </page>
        <page string="Comisiones" id="comisiones">
            <field name="comision_cia" colspan="2"/>
            <field name="comision_vendedor" colspan="2"/>
        </page>
        <page string="Documentos" id="documentos">
            <field name="documentos" colspan="4"/>
        </page>
        <page string="Comentarios" id="comentarios">
            <field name="comentarios" colspan="4"/>
        </page>
        <page string="Otros" id="otros">
            <label name="company"/>
            <field name="company"/>
            <label name="currency"/>
            <field name="currency"/>
        </page>
    </notebook>
</form>

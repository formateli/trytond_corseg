<tryton>
    <data>
        <!-- Poliza Movimiento -->
        <record model="ir.ui.view" id="poliza_movimiento_view_form">
            <field name="model">corseg.poliza.movimiento</field>
            <field name="type">form</field>
            <field name="name">poliza_movimiento_form</field>
        </record>
        <record model="ir.ui.view" id="poliza_movimiento_view_tree">
            <field name="model">corseg.poliza.movimiento</field>
            <field name="type">tree</field>
            <field name="name">poliza_movimiento_tree</field>
        </record>
        <record model="ir.action.act_window" id="act_poliza_movimiento_form">
            <field name="name">Movimientos</field>
            <field name="res_model">corseg.poliza.movimiento</field>
        </record>
        <record model="ir.action.act_window.view" id="act_poliza_movimiento_form_view1">
            <field name="sequence" eval="10"/>
            <field name="view" ref="poliza_movimiento_view_tree"/>
            <field name="act_window" ref="act_poliza_movimiento_form"/>
        </record>
        <record model="ir.action.act_window.view" id="act_poliza_movimiento_form_view2">
            <field name="sequence" eval="20"/>
            <field name="view" ref="poliza_movimiento_view_form"/>
            <field name="act_window" ref="act_poliza_movimiento_form"/>
        </record>

        <record model="ir.action.act_window.domain"
                id="act_poliza_movimiento_form_domain_borrador">
            <field name="name">Borradores</field>
            <field name="sequence" eval="10"/>
            <field name="domain" eval="[('state', '=', 'borrador')]" pyson="1"/>
            <field name="count" eval="True"/>
            <field name="act_window" ref="act_poliza_movimiento_form"/>
        </record>
        <record model="ir.action.act_window.domain"
                id="act_poliza_movimiento_form_domain_procesado">
            <field name="name">Procesados</field>
            <field name="sequence" eval="20"/>
            <field name="domain" eval="[('state', '=', 'procesado')]" pyson="1"/>
            <field name="count" eval="True"/>
            <field name="act_window" ref="act_poliza_movimiento_form"/>
        </record>
        <record model="ir.action.act_window.domain"
                id="act_poliza_movimiento_form_domain_confirmado">
            <field name="name">Confirmados</field>
            <field name="sequence" eval="30"/>
            <field name="domain" eval="[('state', '=', 'confirmado')]" pyson="1"/>
            <field name="count" eval="True"/>
            <field name="act_window" ref="act_poliza_movimiento_form"/>
        </record>
        <record model="ir.action.act_window.domain"
                id="act_poliza_movimiento_form_domain_cancelado">
            <field name="name">Cancelados</field>
            <field name="sequence" eval="40"/>
            <field name="domain" eval="[('state', '=', 'cancelado')]" pyson="1"/>
            <field name="count" eval="True"/>
            <field name="act_window" ref="act_poliza_movimiento_form"/>
        </record>
        <record model="ir.action.act_window.domain"
                id="act_poliza_movimiento_form_domain_all">
            <field name="name">Todos</field>
            <field name="sequence" eval="9999"/>
            <field name="domain"></field>
            <field name="act_window" ref="act_poliza_movimiento_form"/>
        </record>

        <menuitem parent="menu_corseg_poliza" sequence="40"
            action="act_poliza_movimiento_form" id="menu_poliza_movimiento_tree"/>

        <record model="ir.model.access" id="access_poliza_movimiento">
            <field name="model" search="[('model', '=', 'corseg.poliza.movimiento')]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_delete" eval="False"/>
        </record>
        <record model="ir.model.access" id="access_poliza_movimiento_admin">
            <field name="model" search="[('model', '=', 'corseg.poliza.movimiento')]"/>
            <field name="group" ref="group_corseg_admin"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>

        <record model="ir.model.button" id="movimiento_cancel_button">
            <field name="name">cancelar</field>
            <field name="model"
                search="[('model', '=', 'corseg.poliza.movimiento')]"/>
        </record>
        <record model="ir.model.button-res.group"
            id="movimiento_cancel_button_group_corseg_admin">
            <field name="button" ref="movimiento_cancel_button"/>
            <field name="group" ref="group_corseg_admin"/>
        </record>

        <record model="ir.model.button" id="movimiento_procesar_button">
            <field name="name">procesar</field>
            <field name="model"
                search="[('model', '=', 'corseg.poliza.movimiento')]"/>
        </record>
        <record model="ir.model.button-res.group"
            id="movimiento_procesar_button_group_corseg_admin">
            <field name="button" ref="movimiento_procesar_button"/>
            <field name="group" ref="group_corseg_admin"/>
        </record>

        <record model="ir.model.button" id="movimiento_confirmar_button">
            <field name="name">confirmar</field>
            <field name="model"
                search="[('model', '=', 'corseg.poliza.movimiento')]"/>
        </record>
        <record model="ir.model.button-res.group"
            id="movimiento_confirmar_button_group_corseg_admin">
            <field name="button" ref="movimiento_confirmar_button"/>
            <field name="group" ref="group_corseg_admin"/>
        </record>

        <record model="ir.model.button" id="movimiento_borrador_button">
            <field name="name">borrador</field>
            <field name="model"
                search="[('model', '=', 'corseg.poliza.movimiento')]"/>
        </record>
        <record model="ir.model.button-res.group"
            id="movimiento_borrador_button_group_corseg_admin">
            <field name="button" ref="movimiento_borrador_button"/>
            <field name="group" ref="group_corseg_admin"/>
        </record>


        <!-- Certificado -->
        <record model="ir.ui.view" id="certificado_view_form">
            <field name="model">corseg.poliza.certificado</field>
            <field name="type">form</field>
            <field name="name">certificado_form</field>
        </record>
        <record model="ir.ui.view" id="certificado_view_tree">
            <field name="model">corseg.poliza.certificado</field>
            <field name="type">tree</field>
            <field name="name">certificado_tree</field>
        </record>
        <record model="ir.model.access" id="access_certificado">
            <field name="model" search="[('model', '=', 'corseg.poliza.certificado')]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_delete" eval="False"/>
        </record>
        <record model="ir.model.access" id="access_certificado_admin">
            <field name="model" search="[('model', '=', 'corseg.poliza.certificado')]"/>
            <field name="group" ref="group_corseg_admin"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>


        <!-- Extendido -->
        <record model="ir.ui.view" id="extendido_view_form">
            <field name="model">corseg.poliza.certificado.extension</field>
            <field name="type">form</field>
            <field name="name">extendido_form</field>
        </record>
        <record model="ir.ui.view" id="extendido_view_tree">
            <field name="model">corseg.poliza.certificado.extension</field>
            <field name="type">tree</field>
            <field name="name">extendido_tree</field>
        </record>
        <record model="ir.model.access" id="access_extendido">
            <field name="model" search="[('model', '=', 'corseg.poliza.certificado.extension')]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_delete" eval="False"/>
        </record>
        <record model="ir.model.access" id="access_extendido_admin">
            <field name="model" search="[('model', '=', 'corseg.poliza.certificado.extension')]"/>
            <field name="group" ref="group_corseg_admin"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>


        <!-- Certificado Modificacion -->
        <record model="ir.ui.view" id="certificado_modificacion_view_form">
            <field name="model">corseg.poliza.certificado.modificacion</field>
            <field name="type">form</field>
            <field name="name">certificado_modificacion_form</field>
        </record>
        <record model="ir.ui.view" id="certificado_modificacion_view_tree">
            <field name="model">corseg.poliza.certificado.modificacion</field>
            <field name="type">tree</field>
            <field name="name">certificado_modificacion_tree</field>
        </record>
        <record model="ir.model.access" id="access_certificado_modificacion">
            <field name="model" search="[('model', '=', 'corseg.poliza.certificado.modificacion')]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_delete" eval="False"/>
        </record>
        <record model="ir.model.access" id="access_certificado_modificacion_admin">
            <field name="model" search="[('model', '=', 'corseg.poliza.certificado.modificacion')]"/>
            <field name="group" ref="group_corseg_admin"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>
    </data>
</tryton>
